{
  "name": "Roact-Rodux Test Place",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",

      "RoactRodux": {
        "$path": "src"
      },

      "Roact": {
        "$path": "modules/roact/lib"
      },

      "Rodux": {
        "$path": "modules/rodux/lib"
      },

      "TestEZ": {
        "$path": "modules/testez/lib"
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",

      "Run Tests": {
        "$path": "test/runner.server.lua"
      }
    },

    "HttpService": {
      "$className": "HttpService",
      "$properties": {
        "HttpEnabled": true
      }
    },

    "Players": {
      "$className": "Players",
      "$properties": {
        "CharacterAutoLoads": false
      }
    }
  }
}